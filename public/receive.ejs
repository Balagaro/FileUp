<!DOCTYPE html>
<html lang="hu">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width">
    <title>FileUp - Fogadás</title>
    <!-- Google tag (gtag.js) -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=G-WS5TP44M9P"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag(){dataLayer.push(arguments);}
        gtag('js', new Date());

        gtag('config', 'G-WS5TP44M9P');
    </script>
    <link rel="icon" type="image/x-icon" href="docs/logo/favicon.ico">
    <link rel="stylesheet" href="css/main.css">
    <link rel="stylesheet" href="css/receive.css">
</head>

<body>
<div class="block_pass">

</div>
<div class="outpass">
    <div class="inpass">
        <div class="passform" action="">

            <div class="passbar" ><input type="text" placeholder="Adja meg a jelszót!" id="pass1" name="pass1" size="10" > </div>
            <div id="psub1" class="passsubmit">
                <label id="cpass1" for="submitpass" class="codeok">Ok</label>
                <input type="submitpass" autocomplete="off"  id="submitpass"  value="OK">
            </div>
            <div id="psub2" class="passsubmit nonpass">
                <div id="cpass2" class="passok nonpass"><img class="keszpipa readytick" src="docs/assets/readytick.svg"></div>
            </div>
        </div>
    </div>
</div>



<div class="background"></div>
<div class="outer-container">
    <div class="inner-container">
        <div class="leftdiv">
            <div id="menuoszlop" class="menuoszlop">

            </div>

            <div class="dropdown" onclick="toggleMenu(this)">
                <div class="bar1"></div>
                <div class="bar2"></div>
                <div class="bar3"></div>
            </div>

        </div>

        <div class="maindiv">

            <div class="logoline"><a href="/">
                    <img id="mainlogo" src="/docs/logo/fileuplogo.png" alt="FileUP"></a>
            </div>

            <div class="out-box">
                <div class="box">
                    <div class="in-box">
                        <div class="backbutton2">
                            <a href="/"><img src="/docs/assets/backwards.png" alt="Visszagomb">
                            </a>
                        </div>

                    </div>

                    <div class="receivefiles">
                        <div>
                            Fájlok fogadása
                        </div>
                    </div>
                    <div class="out-receivewindow">
                        <div class="receivewindow">
                            <div class="receivebox">
                                <div class="waitingtoreceive">

                                    <div class="waitingtext">
                                        Várakozás a csatlakozásra...

                                        <label class="tick" >
                                            <img class="tickvagyok" src="/docs/assets/tick.svg" alt="pipa">
                                        </label>
                                    </div>

                                </div>

                            </div>

                        </div></div>
                    <div class="out-receivebutt">
                        <div class="receivebutt">
                            <label for="receive" class="reclab">Fogadás</label>
                            <button disabled id="receive"  type="file"></button>
                        </div>
                    </div>
                    <div class="outcode">
                        <div class="incode">
                            <div class="codeform" action="">
                                <div class="codebar" ><input type="text" placeholder="Adja meg a kódot!" autocomplete="off"  id="code" name="code" size="10" > </div>
                                <div id="csub1" class="codesubmit">
                                    <label id="ccode1" for="submit" class="codeok">Ok</label>
                                    <input type="submit" id="submit"  value="OK">
                                </div>
                                <div id="csub2" class="codesubmit nonsub">
                                    <div id="ccode2" class="codeok nonsub"><img class="keszpipa readytick" src="docs/assets/readytick.svg"></div>
                                </div>
                            </div>
                        </div>




                    </div>
                </div>
            </div>
        </div>




        <div class="rightdiv">

            <div><a target=”_blank” href="https://discord.gg/MeqaGhc6QN"><img class="connection-logo"
                                                                              src="/docs/assets/connections/discord.png" alt=""></a></div>
            <div><a target=”_blank” href="https://github.com/Balagaro/FileUp"><img class="connection-logo"
                                                                                   src="/docs/assets/connections/github.png" alt=""></a></div>


        </div>
    </div>

</div>
</div>
<!--<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/downloadjs/1.4.1/download.min.js"></script>-->
<script type="text/javascript" src="modules/downloadjs/download.min.js"></script>
<script type="text/javascript" src="modules/jszip/dist/jszip.min.js"></script>
<!--<script src="https://unpkg.com/jszip/dist/jszip.min.js" type="text/javascript"></script>-->
<!--<script type="text/javascript" src="https://unpkg.com/file-saver/dist/FileSaver.min.js"></script>-->
<script type="text/javascript" src="modules/file-saver/dist/FileSaver.min.js"></script>
<script type="text/javascript" src="modules/socket.io/client-dist/socket.io.js"></script>
<script type="text/javascript" src="receiver.js"></script>

<script>
    function toggleMenu(x) {
        x.classList.toggle("change");
        document.getElementById('menuoszlop').classList.toggle('activeoszlop');
    }
</script>
</body>

</html>